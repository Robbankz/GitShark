{
  "name": "GitShark",
  "version": "0.0.1-alpha.6",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ./src -c .eslintrc.js --ext .js,.jsx,.ts,.tsx",
    "format": "eslint ./src -c .eslintrc.js --fix --ext .js,.jsx,.ts,.tsx",
    "android-send-key": "adb shell input keyevent 82",
    "build-apk": "cd android && ./gradlew assembleRelease",
    "build-aab": "cd android && ./gradlew bundleRelease",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "clean": "react-native-clean-project",
    "clear-xcode-cache": "cd ios && bash ./resetXcode.sh && cd ../",
    "pod-install": "cd ios/ && pod install && cd ..",
    "jetify": "jetify",
    "release": "standard-version"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@oceanbit/styles": "^0.1.3",
    "@react-native-community/hooks": "^2.6.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/picker": "^1.6.6",
    "@react-navigation/material-bottom-tabs": "^5.2.16",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@types/react-redux": "^7.1.8",
    "babel-preset-react-app": "^9.1.2",
    "batteries-not-included": "^0.1.0",
    "buffer": "^5.5.0",
    "core-js": "3",
    "dayjs": "^1.8.33",
    "isomorphic-git": "https://github.com/crutchcorn/isomorphic-git#build",
    "query-string": "^6.13.1",
    "react": "16.13.1",
    "react-content-loader": "^5.1.0",
    "react-dom": "^16.13.1",
    "react-native": "0.63.4",
    "react-native-button-toggle-group": "^1.1.0",
    "react-native-default-preference": "^1.4.2",
    "react-native-dynamic": "^1.0.0",
    "react-native-fs": "https://github.com/oceanbit-dev/react-native-fs.git#all-commands",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-immersive-bars": "^1.0.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-paper": "^4.0.1",
    "react-native-reanimated": "^1.11.0",
    "react-native-safe-area-context": "^3.1.4",
    "react-native-screens": "^2.10.1",
    "react-native-secure-key-store": "^2.0.9",
    "react-native-sqlite-storage": "^5.0.0",
    "react-native-svg": "^12.0.3",
    "react-native-svg-web": "^1.0.8",
    "react-native-vector-icons": "^7.0.0",
    "react-native-video": "^4.4.5",
    "react-native-web": "^0.13.6",
    "react-native-web-linear-gradient": "^1.1.1",
    "react-redux": "^7.2.1",
    "reanimated-bottom-sheet": "^1.0.0-alpha.19",
    "text-encoding": "^0.7.0",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/runtime": "^7.11.2",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react-native": "^0.63.6",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-native-video": "^5.0.1",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.4",
    "hoist-non-react-statics": "^3.3.2",
    "husky": "^4.2.5",
    "jest": "^26.3.0",
    "lint-staged": "^10.2.11",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.0.5",
    "react-native-clean-project": "^3.4.0",
    "react-test-renderer": "16.13.1",
    "standard-version": "^8.0.2",
    "typescript": "^3.9.7"
  },
  "resolutions": {
    "@react-native-community/masked-view": "0.1.10"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run format"
    ]
  }
}
